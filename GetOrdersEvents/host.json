{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "cors": {
    "allowedOrigins": [ "*" ],
    "allowedMethods": [ "GET", "POST", "PUT" ],
    "allowedHeaders": [ "*" ]
  }
}